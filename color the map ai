pip install pytholog


import pytholog as pl


city_color=pl.KnowledgeBase("city_color")
city_color([
             "different(red,green)",
             "different(red,blue)",
             "different(green,red)",
             "different(green,
             "different(blue,green)",
             "coloring(A,M,G,T,F):-different(M,T),different(M,A),different(A,T),different(A,M),different(A,G),different(G,T)"])blue)",
             "different(blue,red)",
             
             city_color.query(pl.Expr("coloring(Alabama,Mississipi,Georgia,Tennessec,Florida)"),cut=True)
